<addon id="script.service.unblockr" name="Unblockr IP Manager" version="0.0.22" provider-name="mark@Unblockr.net">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
  </requires>
<!--
  <extension point="xbmc.python.script" library="unblockr.py">
    <provides>executable</provides>
  </extension>
-->
  <extension point="xbmc.service" library="unblockr.py" start="startup">
  </extension>

  <extension point="xbmc.addon.metadata">
    <summary lang="en">This unblockr.net plugin ensures your registered IP is kept up to date</summary>
    <description lang="en">This plugin will run at startup each time XBMC is run. It calls a special function within Unblockr that will check your current IP address and make sure that Unblockr knows what it is. If you're on the move, you can use the Unblockr Android, or Iphone App to do the same thing. So if you're round at a friend's house and fancy catching the latest Icefilms release, use the app. At home, forget all about it and let us do the work.</description>
    <disclaimer lang="en"></disclaimer>
    <language></language>
    <platform>all</platform>
    <license>GNU GENERAL PUBLIC LICENSE. Version 2, June 1991</license>
    <forum></forum>
    <website>www.unblockr.net</website>
    <email>support@unbloockr.net</email>
    <source></source>
  </extension>
</addon>
